page {
  height: 100%;
  width: 100%;
  background-color: #FFFFFF;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.title {
  height: 25%;
  width: 100%;
  background-color: #1E84D2;
  display: flex;
  align-items: center;
  justify-content: center;
}

.titleContent{
  background-color: #1E84D2;
}

#titlenumber{
  font-size: 80rpx;
}

.content {
  height: 70%;
  width: 100%;
  background-color: #FFFFFF;
  padding-top: 6%;
  padding-left: 5%;
  
}

.trademenu{
  height: 93%;
  width: 90%;
  background-color: FFFFFF;
}

.item{
  height: 19%;
  width: 100%;
  margin-top: 5%;
  background-color: #60C5F1;
  display: flex;
  align-items: center;
  justify-content: center;
}



